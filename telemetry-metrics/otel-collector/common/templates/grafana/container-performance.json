          "expr": "100 * (rate(container_cpu_usage_total_Nanoseconds{fleet_id=\"$fleet_id\", aws_ecs_task_id=\"$task_id\", container_name=~\"$container_name\"}[$__rate_interval]) / rate(container_cpu_usage_system_Nanoseconds{fleet_id=\"$fleet_id\", aws_ecs_task_id=\"$task_id\", container_name=~\"$container_name\"}[$__rate_interval]))",
